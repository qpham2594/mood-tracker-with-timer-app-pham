{{> header}}
<main>
 <h1> All Entries </h1>

  {{#each moodEntries}}
     <div class="entry">
   
    <p>Date: {{this.date}}</p>
    <p>Mood: {{this.mood}}</p>
    <p>Description: {{this.description}}</p>

    <form action="/edit-entry/{{this._id}}" method="GET">
      <button type="submit">Edit</button>
    </form>
  
     <a href="/delete-entry/{{this._id}}"> <button class="delete-button"> Delete </button> </a> 
    
  </div>
  {{/each}}
</main>


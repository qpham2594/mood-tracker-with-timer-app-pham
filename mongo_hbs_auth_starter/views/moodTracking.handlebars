{{> header}}
<main>
 <h1> Welcome to your personalized mood tracker! </h1>

<h2> {{singleQuote}}</h2>

  {{#each moodEntries}}
    <h2>{{this.date}}</h2>
    <p>{{this.mood}}</p>
    <p>{{this.description}}</p>
  {{/each}}

 <button>
  <a href="/new-entry-form"> Add new entry </a> 
 </button>

  <button>
   <a href="/edit-entry/{{userid}}"> Edit an entry </a> 
 </button>
</main>